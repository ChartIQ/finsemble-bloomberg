{
    "components": {
        "Bloomberg Integration Example": {
            "window": {
                "id": "Bloomberg Integration Example",
                "windowType": "native",
                "path": "$fpe-bloombergRoot/src/components/BloombergBridge/bin/Debug/BloombergBridge.exe",
                "arguments": "",
                "autoShow": true,
                "alwaysOnTop": false,
                "resizable": true,
                "showTaskbarIcon": true,
                "addToWorkspace": true,
                "height": 250,
                "width": 300
            },
            "component": {
                "inject": false
            },
            "foreign": {
                "services": {
                    "workspaceService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "persistWindowState": false,
                        "FSBLHeader": true,
                        "showLinker": true
                    },
                    "Toolbar": {
                        "iconClass": "ff-component"
                    }
                }
            }
        },
        "Bloomberg Terminal": {
            "window": {
                "id": "Bloomberg Terminal",
                "name": "Bloomberg Terminal",
                "windowType": "assimilation",
                "path": "C:\\blp\\Wintrv\\wintrv.exe",
                "options": {
                    "autoShow": true
                },
                "addToWorkspace": true
            },
            "component": {
                "inject": false
            },
            "foreign": {
                "services": {
                    "workspaceService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "persistWindowState": false,
                        "FSBLHeader": true,
                        "showLinker": true
                    },
                    "Toolbar": {
                        "iconClass": "ff-component"
                    }
                }
            }
        }
    }
}