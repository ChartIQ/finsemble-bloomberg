{
    "name": "fpe-bloomberg",
    "version": "2.6.11",
    "description": "",
    "main": "",
    "scripts": {
        "start": "node server.js",
        "watch:docs-src": "npx nodemon --exec \"npm run build:docs\" --watch BloombergBridge -e cs,md,yml",
        "docs:dev": "npm-run-all -p watch:docs-src serve:docs",
        "build:docs": "docfx BloombergBridge/docfx_project/docfx.json",
        "serve:docs": "browser-sync start --server BloombergBridge/docfx_project/fpe-bloomberg/ --files BloombergBridge/docfx_project/fpe-bloomberg/"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "4.17.1",
        "cors": "2.8.5"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5",
        "browser-sync": "^2.26.7",
        "nodemon": "^2.0.2"
    },
    "nodemonConfig": {
        "ignore": [
            "BloombergBridge/bin",
            "BloombergBridge/FinsembleResources",
            "BloombergBridge/obj",
            "BloombergBridge/Properties",
            "BloombergBridge/*.config",
            "BloombergBridge/*.csproj",
            "BloombergBridge/docfx_project/fpe-bloomberg",
            "BloombergBridge/docfx_project/obj"
        ]
    }
}
